extend type Query {
    employees: [Employee]
}

type Employee { 
    id: Id!,
    name: String!,
    dateOfBirth: String!,
    city: String!,
    salary: Float!,
    gender: String,
    email: String
}

input CreateEmployeeInput {
    name: String,
    dateOfBirth: String,
    city: String,
    salary: Float,
    gender: String,
    email: String
}

extend type Mutation {
    newEmployee(createEmployeeInput: CreateEmployeeInput!) : Employee!
}